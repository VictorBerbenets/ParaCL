// RUN: %paracl %s |& FileCheck %s -dump-input=fail

//-----------------------------ParaCL code-------------------------------------

ArrSize = 16;
Arr = array(-11, 100, 0, 150, 1488, 8814, 32, 1777, 1945, 32, 1, 1, 2, 2, -1777, 3);

// Buble sort
a = 0;
while (a < ArrSize) {
  b = a + 1;
  while (b < ArrSize) {
    if (Arr[a] > Arr[b]) {
      tmp =  Arr[a];
      Arr[a] = Arr[b];
      Arr[b] = tmp;
    }
    b = b + 1;
  }
  a = a + 1;
}

a = 0;
while (a < ArrSize) {
  print Arr[a];
  a = a + 1;
}

Arr2 = array(-11, 100, 0, 150, 1488, 8814, 32, 1777, 1945, 32, 1, 1, 2, 2, -1777, 3);
// Insertion sort
i = 1;
while(i < ArrSize) {
  NewElem = Arr2[i];
  Location = i - 1;
  while (Location >= 0 && (Arr2[Location] > NewElem)) {
    Arr2[Location + 1] = Arr2[Location];
    Location = Location - 1;
  }
  Arr2[Location + 1] = NewElem;
  i = i + 1;
}

a = 0;
while (a < ArrSize) {
  print Arr2[a];
  a = a + 1;
}

